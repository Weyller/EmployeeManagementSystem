<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pranaykumar.dbmsproject.ShowData">

    <TextView
        android:id="@+id/empl_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:ems="10"
        android:padding="5dp"
        android:textColor="#000"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="Eid"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/empl_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:ems="10"
        android:padding="5dp"
        android:textColor="#000"
        android:layout_below="@+id/empl_id"
        android:text="Name"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/ephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:padding="5dp"
        android:textColor="#000"
        android:layout_below="@+id/empl_name"
        android:layout_marginTop="35dp"
        android:text="Phone"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/egender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:padding="5dp"
        android:textColor="#000"
        android:layout_below="@+id/ephone"
        android:layout_marginTop="35dp"
        android:text="Gender"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/eemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:padding="5dp"
        android:textColor="#000"
        android:inputType="textMultiLine"
        android:layout_below="@+id/egender"
        android:layout_marginTop="35dp"
        android:text="Email"
        android:textSize="25dp"
        android:textIsSelectable="false" />

</RelativeLayout>
